{
  "name": "nuxt-monaco-editor",
  "description": "Integrate monaco-editor with Nuxt (3)",
  "keywords": [
    "monaco-editor",
    "nuxt"
  ],
  "author": "e-chan1007",
  "homepage": "https://e-chan1007.github.io/nuxt-monaco-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-chan1007/nuxt-monaco-editor.git"
  },
  "version": "1.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postpack": "pinst --enable",
    "postinstall": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue . && remark . -q",
    "test": "nuxi test",
    "prepack": "pinst --disable && nuxt-module-build",
    "release": "standard-version",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs:dev": "vuepress dev docs --port 3000",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress serve docs --port 3000"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "vite-plugin-static-copy": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils-edge": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "monaco-editor": "^0.34.1",
    "nuxt": "3.0.0",
    "pinst": "^3.0.0",
    "playwright": "^1.28.1",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "standard-version": "^9.5.0",
    "vitest": "^0.25.6",
    "vuepress": "2.0.0-beta.50"
  },
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  },
  "packageManager": "yarn@3.2.4"
}
